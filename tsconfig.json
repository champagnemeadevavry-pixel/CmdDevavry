{
  "compilerOptions": {
    "target": "ES2020",
  "lib": ["ES2020", "ES2017", "DOM"],
    "module": "ESNext",
  "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
